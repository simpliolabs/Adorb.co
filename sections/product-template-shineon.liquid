<!-- PRODUCT TEMPLATE SHINEON {% render 'shineon-version' %} -->
<!-- DO NOT MODIFY THIS FILE -->
<span data-tags="{{ product.tags | join: ', ' }}"></span>

{% comment %}
SOPP Metafields v2.0
{% endcomment %}
{% assign is_engravable = false %}
{% assign is_engravable_required = false %}
{% assign is_wooden_box = false %}
{% assign pt_id = false %}
{% assign upsell_type = false %}

{% assign product_schema_v2_obj = product.metafields.shineon.schema_v2.value | default: product.metafields.shineon.schema_v2 %}
{% assign pt_id = 'pt_' | append: product_schema_v2_obj.product_template_id %}
{% assign shop_pt_id_obj = shop.metafields.shineon[pt_id].value | default: shop.metafields.shineon[pt_id] %}
{% assign product_cf_obj = shop_pt_id_obj.custom_fields %}
{% for product_cf_id in product_cf_obj %}
  {% assign shop_cf = shop.metafields.shineon[product_cf_id.key].value | default: shop.metafields.shineon[product_cf_id.key] %}
  {% if shop_cf.name == 'font-preview' %}
    {% assign font_preview_feature = shop_cf %}
  {% endif %}
  {% if shop_cf.name contains 'font-family-preview-so' %}
    {% assign font_family_preview_class = shop_cf %}
  {% endif %}
{% endfor %}
{% if font_preview_feature %}
  {{ font_preview_feature.html_chunk }}
{% endif %}
{% assign upsell_type = shop_pt_id_obj.upsell_type %}
{% assign is_upsell_popup_visible = shop_pt_id_obj.is_upsell_popup_visible %}
{% for option in shop_pt_id_obj.options.list %}
  {% if option == upsell_type %}
    {% assign option_upsell_index = forloop.index %}
  {% endif %}
  {% if forloop.index == 2 %}
    {% assign option2 = option %}
  {% endif %}
  {% if forloop.index == 2 and option == 'engraving' %}
    {% assign option2_engraving = true %}
  {% endif %}
{% endfor %}
{% assign is_engravable = shop_pt_id_obj.engravable %}
{% assign is_engravable_required = shop_pt_id_obj.engraving_required %}
{% comment %}
/. SOPP Metafields v2.0
{% endcomment %}

{% case shop.locale %}
  {% when 'fr' %}
    {% assign coordinate_diff_x = -444 %}
    {% assign coordinate_diff_y = 0 %}
  {% when 'es' %}
    {% assign coordinate_diff_x = -888 %}
    {% assign coordinate_diff_y = 0 %}
  {% when 'de' %}
    {% assign coordinate_diff_x = 0 %}
    {% assign coordinate_diff_y = -418 %}
  {% when 'nl' %}
    {% assign coordinate_diff_x = -444 %}
    {% assign coordinate_diff_y = -418 %}
  {% else %}
    {% assign coordinate_diff_x = 0 %}
    {% assign coordinate_diff_y = 0 %}
{% endcase %}
<style type="text/css">
.theme-shineon-sprite.guarantee-100_grande {
  background-position: {{ coordinate_diff_x | plus: -35 }}px {{ coordinate_diff_y | plus: 0 }}px;
}
.theme-shineon-sprite.guarantee-60-day-returns_grande {
  background-position: {{ coordinate_diff_x | plus: -141 }}px {{ coordinate_diff_y | plus:  -7 }}px;
}
.theme-shineon-sprite.easy-returns_grande {
  background-position: {{ coordinate_diff_x | plus: -239 }}px {{ coordinate_diff_y | plus:  3 }}px;
}
.theme-shineon-sprite.fast-delivery_grande {
  background-position: {{ coordinate_diff_x | plus: -354 }}px {{ coordinate_diff_y | plus:  -10 }}px;
}
.theme-shineon-sprite.ordering_grande {
  background-position: {{ coordinate_diff_x | plus: -6 }}px {{ coordinate_diff_y | plus:  -106 }}px;
}
.ribbon-offer {
  background-position: {{ coordinate_diff_x | plus: -6 }}px {{ coordinate_diff_y | plus:  -369 }}px;
}
