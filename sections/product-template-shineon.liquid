</style>

{% comment %}
Regular delivery info
{% endcomment %}
{% assign warranty_prod = all_products['warranty'] %}
{% if warranty_prod.title == '' %}
  {% assign warranty_prod = all_products['2-year-protection-plan'] %}
{% endif %}
{% assign tag_delayed = false %}
{% assign free_shipping = false %}
{% assign copy_delivery_details = 'description_delivery_details_html' | t %}
{% assign warranty_tag = false %}
{% assign cro_design_product = false %}
{% assign cro_design_product_for_ipcs = 'cro_design_product_ipcs' | t %}
{% assign cro_design_product_for_ipcs_arr = cro_design_product_for_ipcs | split: ', ' %}
{% assign prod_id = '' %}
{% assign two_toned_title_for = "PT-1103, PT-964, PT-986, PT-935, PT-798, PT-1348, PT-1163, PT-1089, PT-1501, PT-1351" %}
{% assign lux_and_two_toned_title_for_ipcs = "30084, 30025, 30011, 070, 30028, 30098, 30089" %}
{% assign lux_and_two_toned_title_for_ipcs_arr = lux_and_two_toned_title_for_ipcs | split: ', ' %}
{% assign is_ready_for_3344 = false %}
{% assign is_metal_art_prod = false %}

{% for tag in product.tags %}
  {% comment %}
  Free Shipping feature
  {% endcomment %}
  {% if two_toned_title_for contains tag and shop.metafields.shineon.is_shineon_brand == true and shop.metafields.shineon.is_shineon_brand != blank %}
    {% assign is_ready_for_3344 = true %}
  {% endif %}
  {% if tag contains 'PROD-' %}
    {% assign prod_id = tag | replace: 'PROD-', 'Product ID: #' %}
  {% endif %}
  {% if shop.metafields.shineon.is_shineon_brand == true and shop.metafields.shineon.is_shineon_brand != blank %}
    {% if tag == settings.free_shipping_tag %}
      {% assign free_shipping = true %}
    {% endif %}
  {% endif %}
  {% if tag == section.settings.tag_switch_delayed %}
    {% comment %}
    DELAYED tag switch
    {% endcomment %}
    {% assign tag_delayed = true %}
    {% assign copy_delivery_details = section.settings.copy_delivery_details_delayed %}
  {% endif %}
  {% if tag == section.settings.warranty_tag %}
    {% assign warranty_tag = true %}
  {% endif %}
{% endfor %}

{% assign so_current_variant = product.selected_or_first_available_variant %}
{% assign product_form_id = 'so-product-form-' | append: section.id | append: '-' | append: product.id %}

{% assign has_maxlength = false %}
{% assign so_current_variant_generated_properties = so_current_variant.metafields.shineon.generated_properties.value | default: so_current_variant.metafields.shineon.generated_properties %}
{% assign inputs = so_current_variant_generated_properties.inputs %}
{% assign so_engraving_accessory = so_current_variant_generated_properties.inputs[0].class %}
{% assign is_roman_numeral_date = false %}
{% assign is_roman_numeral_map = false %}
{% assign is_roman_style = false %}
{% for input in inputs %}
  {% assign variant_accessory_quantity = input.name | split: '-' | last %}
  {% if variant.metafields.shineon.ipcs contains 'E060S' or variant.metafields.shineon.ipcs contains 'E060SG' or variant.metafields.shineon.attachment contains 'sweetest-{{ variant_accessory_type }}s-necklace' or variant.metafields.shineon.ipcs contains 'C30102' %}
    {% assign variant_accessory_type = input.name | split: '-' | first %}
  {% endif %}
  {% if input.class == 'so-roman-numeral-date' %}
    {% assign is_roman_numeral_date = true %}
    {% assign is_roman_style = true %}
  {% endif %}
  {% if input.class == 'so-coordinates' %}
    {% assign is_roman_numeral_map = true %}
    {% assign is_roman_style = true %}
  {% endif %}
  {% if input.maxlength %}
    {% assign has_maxlength = true %}
  {% endif %}
{% endfor %}

{% assign no_image_url = 'https://cdn.shopify.com/s/assets/no-image-2048-5e88c1b20e087fb7bbe9a3771824e743c244f437e4f8ba93bbf7b11b53f7824c_1024x1024@2x.gif' %}

{% assign has_engraving = false %}
{% assign engraving_lines = so_current_variant.metafields.shineon.engravings %}
{% assign has_engraving_required = false %}
{% assign has_silver_variants = false %}
{% assign has_gold_variants = false %}
{% assign has_multiple_metals = true %}
{% assign metal_swatch_view = section.settings.option1_metal_swatches %}
{% assign metal_swatch_svg = section.settings.metal_swatch_svg %}
{% assign is_cross = false %}
{% assign is_wooden_box = false %}
{% if upsell_type == 'box' %}
  {% assign is_wooden_box = true %}
{% endif %}

{% assign upsell_option = false %}

{% assign add_upsell_modal = false %}
{% assign sweetest_product_design = false %}
{% assign sided_necklace = false %}
{% assign is_cross_with_engraving = false %}

{% assign maybe_gold = false %}

{% if product.options_with_values.size > 1 %}
  {% assign is_options_detected = false %}
{% endif %}

{% if product.metafields.shineon.uploadable != 1 %}
  {% assign variants_have_lux_box = 0 %}
  {% comment %} GET ENGRAVED AND GOLD FEATURE IMAGES {% endcomment %}
  {% for variant in product.variants %}
    {% if variant.metafields.shineon.ipcs contains 'E035T' %}
      {% assign sided_necklace = true %}
    {% endif %}
    {% if variant.metafields.shineon.ipcs contains 'G' and product.variants.size > 1 %}
      {% assign maybe_gold = true %}
    {% endif %}
    {% if variant.metafields.shineon.ipcs contains 'E060S' or variant.metafields.shineon.ipcs contains 'E060SG' or variant.metafields.shineon.attachment contains 'sweetest-{{ variant_accessory_type }}s-necklace' or variant.metafields.shineon.ipcs contains 'C30102' %}
      {% assign sweetest_product_design = true %}
    {% endif %}
    {% if sweetest_product_design != true %}
      {% if variant.metafields.shineon.ipcs contains 'TG' and product.options_with_values.size == 1 %}
        {% assign add_upsell_modal = variant %}
      {% endif %}
    {% endif %}
    {% if variant.metafields.shineon.ipcs contains '-WOOD' %}
      {% assign variants_have_lux_box = variants_have_lux_box | plus: 1 %}
      {% assign has_box = true %}
      {% assign box_option_index = 1 %}
      {% assign box_option = 'option2' %}
      {% if product.options_with_values.size > 1 %}
        {% assign is_options_detected = true %}
      {% endif %}
      {% if variant.metafields.shineon.gold == 1 %}
        {% assign wooden_box_variant_price_gold = variant.price %}
        {% assign add_upsell_modal_gold = variant %}
      {% else %}
        {% assign wooden_box_variant_price_not_gold = variant.price %}
        {% assign add_upsell_modal_other = variant %}
      {% endif %}
    {% endif %}
    {% if variant.metafields.shineon.attachment contains 'cross' and variant.metafields.shineon.shape == 'cross' %}
      {% assign is_cross = variant %}
    {% endif %}
    {% if variant.metafields.shineon.ipcs contains 'E070T' %}
      {% assign is_cross_with_engraving = variant %}
    {% endif %}
    {% if variant.metafields.shineon.engravable == 1 %}
      {% assign has_engraving = true %}
      {% assign is_options_detected = true %}
      {% if inputs[0].required == 1 %}
        {% assign has_engraving_required = true %}
      {% endif %}
    {% endif %}
    {% if variant.metafields.shineon.engravable == 0 or variant.metafields.shineon.gold == 1 and sweetest_product_design != true %}
      {% if variant.metafields.shineon.gold == 1 or variant.title contains '18' %}
        {% if product.options_with_values.size == 1 %}
          {% assign add_upsell_modal = variant %}
        {% endif %}
      {% endif %}
    {% endif %}
    {% if variant.metafields.shineon.metal == 'other' %}
      {% unless variant.title contains '18' %}
        {% assign regular_variant_id = variant.id %}
      {% endunless %}
    {% else %}
      {% if variant.metafields.shineon.gold != 1 %}
        {% assign regular_variant_id = variant.id %}
      {% endif %}
    {% endif %}
    {% comment %} 3507 tweaks for Metal Art products {% endcomment %}
    {% if variant.metafields.shineon.ipcs contains 'M0' %}
      {% assign is_metal_art_prod = true %}
    {% endif %}
  {% endfor %}
{% endif %}

{% assign is_all_variants_have_lux_box = false %}
{% if variants_have_lux_box == product.variants.size %}
  {% assign is_all_variants_have_lux_box = true %}
{% endif %}

{% assign birthstone_multiple_quantity = '' %}
{% assign is_product_birthstone = false %}
{% for input in inputs %}
  {% if input.field_type == 'custom' %}
    {% if input.id contains 'birthstone' and is_product_birthstone == false %}
      {% assign is_product_birthstone = inputs[forloop.index0] %}
    {% endif %}
  {% endif %}
  {% if input.field_type == 'birthstone' %}
    {% assign is_product_birthstone = true %}
    {% if input.name contains 'Multiple' %}
      {% assign product_birthstone_multiple = true %}
    {% endif %}
  {% endif %}
  {% if input.field_type == 'engraving' %}
    {% if input.id contains 'birthstone' %}
      {% assign product_birthstone_engravable = true %}
      {% assign quantity = input.name | split: 'engraving-' | last %}
      {% if is_product_birthstone == false %}
        {% assign is_product_birthstone = inputs[forloop.index0] %}
      {% endif %}
    {% endif %}
  {% endif %}
{% endfor %}

{% for variant in product.variants %}
  {% if variant.metafields.shineon.engravable == 1 %}
    {% assign has_engraving = true %}
    {% assign is_options_detected = true %}
    {% if inputs[0].required == 1 %}
      {% assign has_engraving_required = true %}
    {% endif %}
  {% endif %}
{% endfor %}

{% assign engraving_option = 'Not Found' %}
{% if product.options_with_values.size > 1 and has_engraving == true %}
  {% if inputs[0].required == 1 %}
    {% assign has_engraving_required = true %}
  {% endif %}
  {% assign engraving_option_index = 1 %}
  {% assign engraving_option = 'option2' %}
{% endif %}
{% for option in product.options_with_values %}
  {% if option.name == 'Box Choice' %}
    {% assign has_box = true %}
    {% assign is_options_detected = true %}
  {% endif %}
{% endfor %}

{% if template contains 'shineon-e-and-g-upsell' %}
  {% assign is_template_optimized = true %}
{% else %}
  {% assign is_template_optimized = false %}
{% endif %}

{% if is_template_optimized == true %}
  {% assign engraving_upsell_price = 0 %}
  {% assign gold_upsell_price = 0 %}
{% endif %}

{% assign engraving_option_yes = 'Yes' %}
{% assign engraving_option_no = 'No' %}
{% assign box_option_yes = 'Luxury Box' %}
{% assign box_option_no = 'Standard Box' %}

{% assign option1_list = "" | split: '--empty-array--' %}
{% assign option2_list = "" | split: '--empty-array--' %}
{% assign option3_list = "" | split: '--empty-array--' %}

{% assign quantity_selector = section.settings.quantity_selector %}
{% assign tag_text = section.settings.tag_text %}


{% assign hidden_variants = "" | split: '--empty-array--' %}
{% assign hidden_images = "" | split: '--empty-array--' %}
{% if so_current_variant.metafields.shineon.type != Nil
    and product.options_with_values.size > 1 %}
  {% assign one_style = 1 %}
{% else %}
  {% assign one_style = 0 %}
{% endif %}

{% if is_template_optimized == true %}
  {% assign base_variant = false %}
{% endif %}

{% assign metals = "" | split: '--empty-array--' %}
{% assign engraved_variants_arr = '' %}
{% assign boxed_variants_arr = '' %}
{% assign is_variants_metal_others = false %}
{% assign metals_others_size = 0 %}
{% assign has_14K_plated_gold_swatch = false %}

{% for variant in product.variants %}
  {% comment %} 3307 CRO'd new PP design {% endcomment %}
  {% if section.settings.design == 'cro-tested' %}
    {% for ipcs_3307 in cro_design_product_for_ipcs_arr %}
      {% if variant.metafields.shineon.ipcs contains ipcs_3307 %}
        {% assign cro_design_product = ipcs_3307 %}
        {% break %}
      {% endif %}
    {% endfor %}
  {% endif %}
  {% if variant.metafields.shineon.engravings > 0 %}
    {% assign has_engraving = true %}
  {% endif %}
  {% if one_style == 1 and product.variants.size >= 8 and variant.metafields.shineon.type != so_current_variant.metafields.shineon.type %}
    {% assign variant_id_arr = variant.id | split: '--array-of-one--' %}
    {% assign variant_image_id_arr = variant.image.id | split: '--array-of-one--' %}
    {% assign hidden_variants = hidden_variants | concat: variant_id_arr %}
    {% assign hidden_images = hidden_images | concat: variant_image_id_arr %}
  {% endif %}

  {% if has_engraving %}
    {% if variant.metafields.shineon.engravable != nil %}
      {% if variant.metafields.shineon.engravable == 1 %}
        {% assign engraving_lines = variant.metafields.shineon.engravings %}
        {% assign engraving_option_yes = variant[engraving_option] %}
      {% elsif variant.metafields.shineon.engravable == 0 %}
        {% assign engraving_option_no = variant[engraving_option] %}
      {% endif %}
    {% endif %}
  {% endif %}

  {% assign variant_ipcs = variant.metafields.shineon.ipcs | downcase %}
  {% assign variant_title = variant.title | downcase %}
  {% if variant_ipcs contains '-wood' %}
    {% assign wooden_box_img = variant.image %}
    {% assign wooden_box_variant_id = variant.id %}
    {% assign wooden_box_variant_price = variant.price %}
    {% assign is_wooden_box = true %}
    {% assign add_upsell_modal = variant %}

    {% comment %} 3514 CRO'd custom titles for lux/oreo boxes {% endcomment %}
    {% for ipcs_3514 in lux_and_two_toned_title_for_ipcs_arr %}
      {% if variant.metafields.shineon.ipcs contains ipcs_3514 and section.settings.cro_change_title_lux_oreo_boxes == true %}
        {% assign is_ready_for_3344 = true %}
      {% endif %}
    {% endfor %}
  {% elsif wooden_box_variant_price_gold or wooden_box_variant_price_not_gold %}
    {% assign standard_box_variant_price = variant.price %}
  {% endif %}
  {% if variant_ipcs contains 'wood' and product.options.size > 1 and product.options_with_values[0].values.size == 1 %}
    {% assign box_icons = true %}
  {% endif %}
  {% if variant_ipcs contains 'wood' and product.options_with_values.size == 1 %}
    {% assign add_upsell_modal = variant %}
    {% if product.options.size == 1 and is_all_variants_have_lux_box != true %}
      {% assign is_wooden_box_icons = true %}
    {% endif %}
  {% else %}
    {% if variant.metafields.shineon.metal == 'other' %}
      {% unless variant.title contains '18' %}
        {% assign regular_variant_id = variant.id %}
      {% endunless %}
    {% else %}
      {% if variant.metafields.shineon.gold != 1 %}
        {% assign regular_variant_id = variant.id %}
      {% endif %}
    {% endif %}
  {% endif %}

  {% if base_variant == false or base_variant.price > variant.price and is_template_optimized == true %}
    {% assign base_variant = variant %}
  {% endif %}

  {% if variant.metafields.shineon.gold == 1 %}
    {% assign has_gold_variants = true %}
    {% assign gold_variant_price = variant.price %}
  {% endif %}

  {% unless variant.metafields.shineon.gold == 1 %}
    {% assign has_silver_variants = true %}
    {% assign not_gold_variant_price = variant.price %}
  {% endunless %}


  {% if variant.metafields.shineon.metal != 'other' %}
    {% if variant.metafields.shineon.gold != nil %}
      {% assign metal = variant.metafields.shineon.gold | split: '--array-of-one--' %}
    {% else %}
      {% assign metal = "" | split: '--empty-array--' %}
    {% endif %}
  {% else %}
    {% assign metals_others_size = metals_others_size | plus: 1 %}
  {% endif %}
  {% assign metals = metals | concat: metal %}
  {% assign variant_option1_arr = variant.option1 | split: '--array-of-one--' %}
  {% assign option1_list = option1_list | concat: variant_option1_arr %}
  {% assign variant_option2_arr = variant.option2 | split: '--array-of-one--' %}
  {% assign option2_list = option2_list | concat: variant_option2_arr %}
  {% assign variant_option3_arr = variant.option3 | split: '--array-of-one--' %}
  {% assign option3_list = option3_list | concat: variant_option3_arr %}

  {% if has_engraving and variant[engraving_option] == "Yes" %}
  {% assign engraved_variants_arr = engraved_variants_arr | append: '--next-prop--' | append: variant.metafields.shineon.metal | append: '---' | append: variant.featured_image | append: '---' | append: variant.price %}
  {% endif %}

  {% if has_box == true and product.options.size > 1 and variant.metafields.shineon.ipcs contains "-WOOD" %}
    {% assign boxed_variants_arr = boxed_variants_arr | append: '--next-prop--' | append: variant.metafields.shineon.metal | append: '---' | append: variant.featured_image | append: '---' | append: variant.price %}

    {% if so_current_variant.option1 == variant.option1 and so_current_variant.option2 != variant.option2 %}
      {% assign price_box = variant.price %}
    {% endif %}
  {% endif %}

  {% if variant.metafields.shineon.gold == 1 and variant.metafields.shineon.metal == 'gold14' and product.variants.size < 3 %}
    {% assign has_14K_plated_gold_swatch = true %}
  {% endif %}

{% endfor %}
{% assign engraved_variants_arr_js = engraved_variants_arr | remove_first: '--next-prop--' %}
{% assign engraved_variants_arr = engraved_variants_arr | remove_first: '--next-prop--' | split: '--next-prop--' %}

{% assign boxed_variants_arr_js = boxed_variants_arr | remove_first: '--next-prop--' %}
{% assign boxed_variants_arr = boxed_variants_arr | remove_first: '--next-prop--' | split: '--next-prop--' %}

{% assign hidden_variants_str = hidden_variants | join: ' ' %}

{% assign metals = metals | uniq %}
{% assign option1_list = option1_list | uniq %}
{% assign option2_list = option2_list | uniq %}
{% assign option3_list = option3_list | uniq %}

{% if metals.size > 1 and sweetest_product_design != true %}
  {% assign has_multiple_metals = true %}
{% else %}
  {% assign has_multiple_metals = false %}
{% endif %}

{% if product.variants.size == metals_others_size %}
  {% assign is_variants_metal_others = true %}
{% endif %}

{% if is_template_optimized == true and has_engraving == true %}
  {% comment %} DETERMINE ENGRAVING AND GOLD UP SELL PRICES {% endcomment %}
  {% for variant in product.variants %}
    {% if has_multiple_metals %}
      {% if base_variant.option1 != variant.option1 and base_variant.option2 == variant.option2 and base_variant.option3 == variant.option3 %}
        {% assign gold_upsell_price = variant.price | minus:base_variant.price | abs %}
      {% endif %}
    {% endif %}

    {% if base_variant.option1 == variant.option1 and base_variant.option2 != variant.option2 and base_variant.option3 == variant.option3 %}
      {% assign engraving_upsell_price = variant.price | minus:base_variant.price | abs %}
    {% endif %}
  {% endfor %}
{% endif %}

{% if section.settings.express_checkout == true %}
    {% assign use_shineon_cart = true %}
{% elsif product.metafields.shineon.uploadable == 1 and section.settings.use_shineon_cart_page_bu == true %}
    {% assign use_shineon_cart = true %}
{% elsif section.settings.use_shineon_cart_page_eng == true %}
    {% assign use_shineon_cart = true %}
{% else %}
    {% assign use_shineon_cart = false %}
{% endif %}

{% assign hide_tabs_if_left_side_or_hidden = false %}
{% assign show_tabs_if_right_side = false %}
{% if section.settings.show_about_us_review_list != true %}
  {% if section.settings.content_disposition == 'left_side' or section.settings.content_disposition == 'hidden' %}
    {% assign hide_tabs_if_left_side_or_hidden = true %}
  {% endif %}

  {% if section.settings.content_disposition == 'right_side_expanded' or section.settings.content_disposition == 'right_side_collapsed' %}
    {% assign show_tabs_if_right_side = true %}
  {% endif %}
{% endif %}

{% if shop.metafields.shineon.is_shineon_brand == true and shop.metafields.shineon.is_shineon_brand != blank %}
  {% include 'request-host' %}
{% endif %}

{% include 'product-shineon-custom-styles' %}

{% if product.metafields.shineon.uploadable == 1 %}
  {% include 'product-shineon-scripts' with 'nav-progress-steps' %}
  {% include 'product-shineon-scripts' with 'default-images' %}
{% endif %}

{% unless engraved_variants_arr.size > 0 %}
  {% assign has_engraving = false %}
{% endunless %}
{% if upsell_type == 'engraving' %}
  {% assign has_engraving = true %}
{% endif %}
{% if has_engraving == true %}
  {% assign upsell_option = 'upsell_engraving' %}
{% elsif has_14K_plated_gold_swatch == false and has_box == true or is_wooden_box == true %}
  {% assign upsell_option = 'upsell_box' %}
{% elsif has_multiple_metals == true or maybe_gold == true and sweetest_product_design != true %}
  {% assign upsell_option = 'upsell_material' %}
{% else %}
  {% assign upsell_option = false %}
  {% assign is_options_detected = false %}
{% endif %}

{% comment %}
