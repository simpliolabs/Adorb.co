<script>
    $(document).ready(function() {
      
        setTimeout(function() {
         
          var isTextsInputed = true;
          var requiredTextInputs = $('input[aria-required="true"].tee__input--text').filter(function() {
            return $(this).parent().parent().css('display') !== 'none';
          });

          requiredTextInputs.each(function() {
            var inputVal = $(this).val();
            console.log("INPUT TEXT:" + inputVal);
            if(inputVal == "" || inputVal == null){
              isTextsInputed=false;
            }
          });

          var checker = document.getElementById('confirmed');
          var sendbtn = document.getElementById('sendNewSms');
            // when unchecked or checked, run the function
            checker.onchange = function() {
                if (this.checked) {
                    $('.btn-cart').removeAttr('disabled');
                } else {
                    $('.btn-cart').attr('disabled', true);
                }
            }

          $('#teeAtcButton').click(function() {
